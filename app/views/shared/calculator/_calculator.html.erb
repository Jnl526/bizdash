<div id="calculator" class="table-responsive">
  <form name ="form">
    <input type="text" class="textview" name="textview" placeholder="0">
  </form>
  <table>
    <tr>
      <td><input type="button" value="C" class="operators" onclick="clean()"></td>
      <td><input type="button" value="<" class="operators" onclick="back()"></td>
      <td><input type="button" value="/" class="operators" onclick="insert('/')"></td>
      <td><input type="button" value="*" class="operators" onclick="insert('*')"></td>
    </tr>
    <tr>
      <td><input type="button" value="7" class="operators" onclick="insert(7)"></td>
      <td><input type="button" value="8" class="operators" onclick="insert(8)"></td>
      <td><input type="button" value="9" class="operators" onclick="insert(9)"></td>
      <td><input type="button" value="-" class="operators" onclick="insert('-')"></td>
    </tr>
    <tr>
      <td><input type="button" value="4" class="operators" onclick="insert(4)"></td>
      <td><input type="button" value="5" class="operators" onclick="insert(5)"></td>
      <td><input type="button" value="6" class="operators" onclick="insert(6)"></td>
      <td><input type="button" value="+" class="operators"  onclick="insert('+')"></td>
    </tr>
    <tr>
      <td><input type="button" value="1" class="operators" onclick="insert(1)"></td>
      <td><input type="button" value="2" class="operators" onclick="insert(2)"></td>
      <td><input type="button" value="3" class="operators" onclick="insert(3)"></td>
      <td rowspan=5><input type="button" value="=" class="operators equal" onclick="equal()"></td>
    </tr>
    <tr>
      <td colspan=2><input type="button" value="0" class="operators zero" onclick="insert(0)"></td>
      <td><input type="button" value="." class="operators"  onclick="insert('.')"></td>
    </tr>
    </table>
</div>